  "devDependencies": {
    "@iml/flow-highland": "^4.3.0",
    "@iml/flow-jasmine": "1.6.1",
    "@iml/jasmine-n-matchers": "2.1.1",
    "babel-eslint": "8.2.3",
    "babel-plugin-external-helpers": "6.22.0",
    "babel-plugin-transform-class-properties": "6.24.1",
